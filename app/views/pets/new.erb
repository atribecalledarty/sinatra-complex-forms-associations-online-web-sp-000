<h1>Create a new Pet</h1>

<form method="POST" action="/pets">
  <label>Name: </label>
  <input type="text" name="pet_name">
  
  <label>Choose existing owner:</label>
  <% @owners.each do |owner| %>
    <input type="checkbox" name="owner_id" value="<%= owner.id %>"
  <% end %>
  
  
  
  
  
</form>